{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "apiVersions": {
      "type": "object",
      "metadata": {
        "description": "Api versions object"
      }
    },
    "templateUrls": {
      "type": "object",
      "metadata": {
        "description": "Nested template URL's"
      }
    },
    "virtualMachineSettings": {
      "type": "object",
      "metadata": {
        "description": ""
      }
    }
  },
  "variables": {
    "deploymentApiVersion": "[parameters('apiVersions').deploymentApiVersion]",
    "virtualMachineSettings": "[parameters('virtualMachineSettings')]",
    "virtualMachineNodeTemplateUrl": "[parameters('templateUrls').virtualMachineNodeTemplateUrl]"
  },
  "resources": [
    {
      "name": "virtualMachineDeployment",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "[variables('deploymentApiVersion')]",
      "comments": "Virtual machine deployment",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[variables('virtualMachineNodeTemplateUrl')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "virtualMachineSettings": {
            "value": "[variables('virtualMachineSettings')]"
          }
        }
      }
    }
  ]
}